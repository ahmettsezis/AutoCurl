pipeline {
    agent any

    stages {
        stage('Container Prune 1') {
            steps {
                sh "docker container prune -f"
            }
        }
        stage('Run Python Container') {
            steps {
                 withCredentials([string(credentialsId: 'accessK', variable: 'access'), string(credentialsId: 'secretK', variable: 'secret')]) {
                     sh "docker run --env AWS_ACCESS_KEY_ID=$access --env AWS_SECRET_ACCESS_KEY=$secret --name container_autocurl ahmettsezis/autocurl_slim"
            }
            }
        }
        stage('Container Prune 2') {
            steps {
                sh "docker container prune -f"
            }
        }
    }
}
